{
  "$type": "module",
  "forms": [
  
    ["def", "newton-method",
      ["lambda", ["f", "f-prime", "x", "h"],
        ["do",
          ["def", "x-next",
            ["-", "x", ["/", ["f", "x"], "f-prime"]]
          ],
          ["if",
            ["<", ["abs", ["-", "x-next", "x"]], "h"],
            "x-next",
            ["newton-method", "f", "f-prime", "x-next", "h"]
          ]
        ]
      ]
    ],

    ["def", "num-deriv",
      ["lambda", ["f", "h"],
        ["lambda", ["x"],
          ["/", ["-", ["f", ["+", "x", "h"]],
                      ["f", ["-", "x", "h"]]],
                ["*", 2, "h"]]
        ]
      ]
    ]
  ]
}